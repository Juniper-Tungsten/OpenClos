{
"l2Report": {
  "devices": [
    {% for device in devices %}    {"id": "{{ device['id'] }}", "name": "{{ device['name'] }}", "family": "{{ device['family'] }}", "role":"{{ device['role'] }}", "status":"{{ device['status'] }}", "statusReason":"{{ device['reason'] }}"}{% if not loop.last %},
    {% endif %}{% endfor %}
  ],
  "links": [
    {% for link in links %}    { "device1": "{{ link['device1'] }}", "port1": "{{ link['port1'] }}", "ip1":"{{ link['ip1'] }}", "device2": "{{ link['device2'] }}", "port2": "{{ link['port2'] }}", "ip2":"{{ link['ip2'] }}", "lldpStatus":"{{ link['lldpStatus'] }}"}{% if not loop.last %},
    {% endif %}{% endfor %}
  ]
}
}